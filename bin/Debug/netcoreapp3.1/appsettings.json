{
  "TDXLambdaConfig": {
    "TcpConfig": {
      "IsEnabled": true,
      "RequestPort": 6151,
      "ReplyPort": 9161,
      "NameSpace": "",
      "IsDynamicAddressingEnabled": false
    },
    "StartupListener": "Bookindotcom:Bookindotcom.StartupListener",
    "Endpoints": [
      {
        "RequestContentType": "application/json",
        "ResponseContentType": "application/json",
        "AssemblyReference": "Bookindotcom:Bookindotcom.HotelSearch:Execute",
        "HttpReference": "POST /api/hotel/search"
      },
      {
        "RequestContentType": "application/json",
        "ResponseContentType": "application/json",
        "AssemblyReference": "Bookindotcom:Bookindotcom.HotelwithRoomDetail:Execute",
        "HttpReference": "POST /api/hotel/details"
      },
      {
        "RequestContentType": "application/json",
        "ResponseContentType": "application/json",
        "AssemblyReference": "Bookindotcom:Bookindotcom.Hotelpolicy:Execute",
        "HttpReference": "POST /api/hotel/policies"
      },
      {
        "RequestContentType": "application/json",
        "ResponseContentType": "application/json",
        "AssemblyReference": "Bookindotcom:Bookindotcom.CheckHotelAvail:Execute",
        "HttpReference": "POST /api/hotel/checkavailability"
      },
      {
        "RequestContentType": "application/json",
        "ResponseContentType": "application/json",
        "AssemblyReference": "Bookindotcom:Bookindotcom.HotelReservation:Execute",
        "HttpReference": "POST /api/hotel/reservation"
      },
      {
        "RequestContentType": "application/json",
        "ResponseContentType": "application/json",
        "AssemblyReference": "Bookindotcom:Bookindotcom.HotelViewReservation:Execute",
        "HttpReference": "POST /api/hotel/viewreservation"
      },
      {
        "RequestContentType": "application/json",
        "ResponseContentType": "application/json",
        "AssemblyReference": "Bookindotcom:Bookindotcom.HotelCancelReservation:Execute",
        "HttpReference": "POST /api/hotel/cancelreservation"
      }
    ]
  }
}